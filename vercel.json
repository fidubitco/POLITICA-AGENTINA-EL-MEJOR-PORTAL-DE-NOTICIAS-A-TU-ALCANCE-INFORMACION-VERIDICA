{
  "version": 2,
  "framework": null,
  "buildCommand": "pnpm install && pnpm build",
  "outputDirectory": "public",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/(favicon\\.ico|favicon\\.png|favicon-16x16\\.png|favicon-32x32\\.png|apple-touch-icon\\.png|manifest\\.json|robots\\.txt|sitemap\\.xml|sw\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(assets|images|locales)/(.*)$",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(favicon|logo|manifest|robots|sitemap).*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}